{
  "/home/dexp/Music/coffee-shop-api/serverless.yml": {
    "versionFramework": "4.17.2",
    "servicePath": "/home/dexp/Music/coffee-shop-api/serverless.yml",
    "serviceConfigFileName": "serverless.yml",
    "service": {
      "service": "coffee-shop-api",
      "frameworkVersion": "4",
      "provider": {
        "name": "aws",
        "runtime": "nodejs18.x",
        "region": "${opt:region, 'us-east-1'}",
        "stage": "dev",
        "environment": {
          "COFFEE_TABLE": "${self:service}-${self:provider.stage}-coffees",
          "STAGE": "${self:provider.stage}"
        },
        "iam": {
          "role": {
            "statements": [
              {
                "Effect": "Allow",
                "Action": [
                  "dynamodb:Query",
                  "dynamodb:Scan",
                  "dynamodb:GetItem",
                  "dynamodb:PutItem",
                  "dynamodb:UpdateItem",
                  "dynamodb:DeleteItem"
                ],
                "Resource": {
                  "Fn::GetAtt": [
                    "CoffeeTable",
                    "Arn"
                  ]
                }
              }
            ]
          }
        }
      },
      "plugins": [
        "serverless-offline"
      ],
      "functions": {
        "createCoffee": {
          "handler": "src/handlers/createCoffee.handler",
          "events": [
            {
              "http": {
                "path": "coffees",
                "method": "post",
                "cors": true
              }
            }
          ]
        },
        "getCoffees": {
          "handler": "src/handlers/getCoffees.handler",
          "events": [
            {
              "http": {
                "path": "coffees",
                "method": "get",
                "cors": true
              }
            }
          ]
        },
        "getCoffee": {
          "handler": "src/handlers/getCoffee.handler",
          "events": [
            {
              "http": {
                "path": "coffees/{id}",
                "method": "get",
                "cors": true
              }
            }
          ]
        },
        "updateCoffee": {
          "handler": "src/handlers/updateCoffee.handler",
          "events": [
            {
              "http": {
                "path": "coffees/{id}",
                "method": "put",
                "cors": true
              }
            }
          ]
        },
        "deleteCoffee": {
          "handler": "src/handlers/deleteCoffee.handler",
          "events": [
            {
              "http": {
                "path": "coffees/{id}",
                "method": "delete",
                "cors": true
              }
            }
          ]
        }
      },
      "resources": {
        "Resources": {
          "CoffeeTable": {
            "Type": "AWS::DynamoDB::Table",
            "Properties": {
              "TableName": "${self:provider.environment.COFFEE_TABLE}",
              "AttributeDefinitions": [
                {
                  "AttributeName": "id",
                  "AttributeType": "S"
                }
              ],
              "KeySchema": [
                {
                  "AttributeName": "id",
                  "KeyType": "HASH"
                }
              ],
              "BillingMode": "PAY_PER_REQUEST",
              "Tags": [
                {
                  "Key": "Environment",
                  "Value": "${self:provider.stage}"
                },
                {
                  "Key": "Service",
                  "Value": "${self:service}"
                }
              ]
            }
          }
        }
      },
      "custom": {
        "serverless-offline": {
          "httpPort": 3000
        }
      },
      "build": {
        "esbuild": {
          "bundle": true,
          "minify": true,
          "sourcemap": false,
          "external": [
            "aws-sdk",
            "uuid"
          ]
        }
      }
    },
    "provider": {
      "name": "aws",
      "runtime": "nodejs18.x",
      "region": "${opt:region, 'us-east-1'}",
      "stage": "dev",
      "environment": {
        "COFFEE_TABLE": "${self:service}-${self:provider.stage}-coffees",
        "STAGE": "${self:provider.stage}"
      },
      "iam": {
        "role": {
          "statements": [
            {
              "Effect": "Allow",
              "Action": [
                "dynamodb:Query",
                "dynamodb:Scan",
                "dynamodb:GetItem",
                "dynamodb:PutItem",
                "dynamodb:UpdateItem",
                "dynamodb:DeleteItem"
              ],
              "Resource": {
                "Fn::GetAtt": [
                  "CoffeeTable",
                  "Arn"
                ]
              }
            }
          ]
        }
      }
    },
    "dashboard": {
      "isEnabledForService": false,
      "requiredAuthentication": false,
      "orgFeaturesInUse": null,
      "orgObservabilityIntegrations": null,
      "serviceAppId": null,
      "serviceProvider": null,
      "instanceParameters": null
    },
    "serviceRawFile": "service: coffee-shop-api\n\nframeworkVersion: '4'\n\nprovider:\n  name: aws\n  runtime: nodejs18.x\n  region: ${opt:region, 'us-east-1'}\n  stage: ${opt:stage, 'dev'}\n  environment:\n    COFFEE_TABLE: ${self:service}-${self:provider.stage}-coffees\n    STAGE: ${self:provider.stage}\n  iam:\n    role:\n      statements:\n        - Effect: Allow\n          Action:\n            - dynamodb:Query\n            - dynamodb:Scan\n            - dynamodb:GetItem\n            - dynamodb:PutItem\n            - dynamodb:UpdateItem\n            - dynamodb:DeleteItem\n          Resource: !GetAtt CoffeeTable.Arn\n\nplugins:\n  - serverless-offline\n\nfunctions:\n  createCoffee:\n    handler: src/handlers/createCoffee.handler\n    events:\n      - http:\n          path: coffees\n          method: post\n          cors: true\n\n  getCoffees:\n    handler: src/handlers/getCoffees.handler\n    events:\n      - http:\n          path: coffees\n          method: get\n          cors: true\n\n  getCoffee:\n    handler: src/handlers/getCoffee.handler\n    events:\n      - http:\n          path: coffees/{id}\n          method: get\n          cors: true\n\n  updateCoffee:\n    handler: src/handlers/updateCoffee.handler\n    events:\n      - http:\n          path: coffees/{id}\n          method: put\n          cors: true\n\n  deleteCoffee:\n    handler: src/handlers/deleteCoffee.handler\n    events:\n      - http:\n          path: coffees/{id}\n          method: delete\n          cors: true\n\nresources:\n  Resources:\n    CoffeeTable:\n      Type: AWS::DynamoDB::Table\n      Properties:\n        TableName: ${self:provider.environment.COFFEE_TABLE}\n        AttributeDefinitions:\n          - AttributeName: id\n            AttributeType: S\n        KeySchema:\n          - AttributeName: id\n            KeyType: HASH\n        BillingMode: PAY_PER_REQUEST\n        Tags:\n          - Key: Environment\n            Value: ${self:provider.stage}\n          - Key: Service\n            Value: ${self:service}\n\ncustom:\n  serverless-offline:\n    httpPort: 3000\n\nbuild:\n  esbuild:\n    bundle: true\n    minify: true\n    sourcemap: false\n    external:\n      - 'aws-sdk'\n      - 'uuid'\n\n",
